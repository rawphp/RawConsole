{
    "name": "rawphp/raw-console",
    "type": "library",
    "description": "RawConsole is a simple console package used by RawPHP framework and other applications.",
    "keywords": ["console"],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-support": "0.*@dev",
        "rawphp/raw-dispatcher": "0.*@dev",
        "rawphp/raw-code-standards": "1.*@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawConsole\\": "src/RawPHP/RawConsole/"
        }
    },
    "bin": ["bin/console"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
